def word_count(astring):
	wordCount = {}
	for ch in astring.split():
		if ch not in wordCount:
			wordCount[ch] = 1
		else:
			wordCount[ch] += 1
	return wordCount
  
  
  
  
  
import requests
#this code snippet makes a simple get request to pokeapi and returns the status code
def get_status_code(): 
    response = requests.get(http://beta.pokeapi.co/docsv2/pokeapi.co/api/v2//)
    return response.status_code
    
    
    
    
def find_max_min(alist):
    newList = sorted(alist)
    l = [newList[0], newList[-1]]
    return l
    
    

